{
  "name": "nextjs-typescript-effect-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "tsup": "tsup",
    "start": "serve out -l 3000",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev --port 6006",
    "storybook:poll": "CHOKIDAR_USEPOLLING=1 WATCHPACK_POLLING=true storybook dev --port 6006",
    "build-storybook": "storybook build",
    "check-typescript": "tsc --noEmit",
    "watch-typescript": "tsc --noEmit --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:storybook": "vitest run --config vitest.config.ts",
    "top-up-highlights:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/top-up-highlights.ts",
    "top-up-highlights": "node dist/cli/top-up-highlights.mjs",
    "top-up-categories:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/top-up-categories.ts",
    "top-up-categories": "node dist/cli/top-up-categories.mjs",
    "top-up-reviewed-preprints:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/top-up-reviewed-preprints.ts",
    "top-up-reviewed-preprints": "node dist/cli/top-up-reviewed-preprints.mjs",
    "calibrate-reviewed-preprints:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/calibrate-reviewed-preprints.ts",
    "calibrate-reviewed-preprints": "node dist/cli/calibrate-reviewed-preprints.mjs",
    "prune-reviewed-preprints:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/prune-reviewed-preprints.ts",
    "prune-reviewed-preprints": "node dist/cli/prune-reviewed-preprints.mjs",
    "purge-reviewed-preprints:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/purge-reviewed-preprints.ts",
    "purge-reviewed-preprints": "node dist/cli/purge-reviewed-preprints.mjs",
    "backup-cached:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/backup-cached.ts",
    "backup-cached": "node dist/cli/backup-cached.mjs",
    "restore-cached:dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register src/cli/restore-cached.ts",
    "restore-cached": "node dist/cli/restore-cached.mjs",
    "chromatic": "sh ./scripts/run-chromatic.sh"
  },
  "dependencies": {
    "@effect/cli": "^0.72.1",
    "@effect/platform": "^0.93.2",
    "@effect/platform-node": "^0.101.1",
    "@mantine/core": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "effect": "^3.19.3",
    "html-react-parser": "^5.2.10",
    "next": "^16.0.8",
    "react": "^19.2.1",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-themes": "^10.1.6",
    "@storybook/addon-vitest": "^10.0.7",
    "@storybook/nextjs-vite": "^10.0.7",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^4.0.13",
    "@vitest/browser-playwright": "^4.0.13",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.9.6",
    "chromatic": "^13.3.4",
    "dir-compare-cli": "^1.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "storybook": "^10.0.7",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "vitest": "^4.0.13"
  }
}
